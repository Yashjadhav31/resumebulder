{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:server\" \"vite\"",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "build:server": "tsc -p tsconfig.server.json",
    "start:server": "node --enable-source-maps dist/server.js",
    "server": "npm run build:server && npm run start:server",
    "seed": "npm run build:server && node seed.js",
    "seed:ts": "npx ts-node --esm seed.ts",
    "ml-service": "cd ml_service && python app.py",
    "dev:full": "concurrently \"npm:ml-service\" \"npm:server\" \"vite\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@types/dotenv": "^6.1.1",
    "compromise": "^14.14.4",
    "dotenv": "^17.2.3",
    "express-async-handler": "^1.2.0",
    "keyword-extractor": "^0.0.28",
    "lucide-react": "^0.344.0",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.1",
    "multer": "^2.0.2",
    "natural": "^8.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.8.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
